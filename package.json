{
  "name": "langchain-express",
  "version": "1.0.0",
  "description": "Langchain Express Server",
  "main": "index.js",
  "scripts": {
    "rebuild": "del-cli --force dist && npm run build",
    "build": "tsc",
    "start": "node dist/find-node.js $PORT",
    "checkport": "lsof -i :3000",
    "watch:deploy": "nodemon -e ts,js --watch 'src/**/*.ts' --watch 'public/js/*.js' --exec 'npm run build && npm run start'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "ljeff@us.ibm.com",
  "license": "ISC",
  "dependencies": {
    "@ibm-generative-ai/node-sdk": "^2.0.1",
    "chromadb": "^1.8.1",
    "cors": "^2.8.5",
    "d3-dsv": "^2.0.0",
    "express": "^4.18.3",
    "express-fileupload": "^1.4.3",
    "jsonfile": "^6.1.0",
    "langchain": "^0.1.26",
    "rxjs": "^7.8.1",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.25",
    "del-cli": "^5.1.0",
    "dotenv": "^16.4.5",
    "nodemon": "^3.1.0",
    "typescript": "^5.4.2"
  }
}
